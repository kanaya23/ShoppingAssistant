# Shopping Assistant Web Server Configuration
# Copy this to .env and fill in your values

# API Keys (Required)
GEMINI_API_KEY=your_gemini_api_key_here
SERPER_API_KEY=your_serper_api_key_here

# Server Config
HOST=0.0.0.0
PORT=5000
DEBUG=false

# SSL Config - Set to false for easier setup (no certificate issues)
# Only enable if you have proper Let's Encrypt certificates
USE_SSL=false
SSL_CERT=certs/cert.pem
SSL_KEY=certs/key.pem

# Optional Authentication Token (leave empty for no auth)
AUTH_TOKEN=
